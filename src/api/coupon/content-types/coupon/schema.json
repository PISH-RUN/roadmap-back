{
  "kind": "collectionType",
  "collectionName": "coupons",
  "info": {
    "singularName": "coupon",
    "pluralName": "coupons",
    "displayName": "coupon",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "code": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "capacity": {
      "type": "integer",
      "required": true,
      "default": 1
    },
    "used": {
      "type": "integer",
      "default": 0,
      "required": true
    },
    "amount": {
      "type": "integer",
      "required": false,
      "min": 1
    },
    "expiresAt": {
      "type": "datetime"
    },
    "percent": {
      "type": "integer",
      "max": 100,
      "min": 1
    },
    "perUser": {
      "type": "integer"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "coupons"
    },
    "onlySubscriptions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::subscription.subscription"
    }
  }
}
